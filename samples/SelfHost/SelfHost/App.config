<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

	<system.diagnostics>
		<trace autoflush="true"
			   indentsize="4">
			<listeners>
				<add name="myListener"
					 type="System.Diagnostics.TextWriterTraceListener"
					 initializeData="Trace.log" />
				<remove name="Default" />
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>